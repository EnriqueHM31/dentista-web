{
    "version": 2,
    "builds": [
        {
            "src": "backend/build/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "backend/build/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "backend/build/routes/(.*)",
            "dest": "backend/build/index.js"
        }
    ]
}